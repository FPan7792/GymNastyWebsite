  export NODE_PATH="$NODE_PATH:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/1.SiteGymDepart/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/1.SiteGymDepart/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/1.SiteGymDepart/node_modules/.pnpm/typescript@4.6.4/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/1.SiteGymDepart/node_modules/.pnpm/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/1.SiteGymDepart/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/node_modules:/Users/franck/Desktop/FORMATION/node_modules:/Users/franck/Desktop/node_modules:/Users/franck/node_modules:/Users/node_modules:/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/1.SiteGymDepart/node_modules/typescript/bin/node_modules:/Users/franck/Desktop/FORMATION/UDEMY/JavaScript/3_SITES_EN_JS/CodeSourceCours3Sites-master/1.DossierDepart/1.SiteGymDepart/node_modules/typescript/node_modules"
